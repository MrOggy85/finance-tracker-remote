{
  "name": "finance-tracker",
  "version": "0.1.0",
  "private": true,
  "main": "src/main.ts",
  "build": {
    "appId": "com.otaku.finance-tracker",
    "files": [
      "arc/**/*",
      "node_modules/**/*",
      "package.json"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint src/**/*",
    "postinstall": "install-app-deps",
    "dev": "concurrently -k \"BROWSER=none npm start\" \"npm:electron\"",
    "electron": "tsc --project electron/tsconfig.json && electron ./build/electron/main.js",
    "electron:build": "tsc --project electron/tsconfig.json",
    "electron:watch": "tsc --watch --project electron/tsconfig.json"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.7.2",
    "bootstrap": "^5.1.3",
    "chart.js": "^3.7.1",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "electron": "^17.1.0",
    "electron-is-dev": "^2.0.0",
    "pg": "^8.7.3",
    "react": "^17.0.2",
    "react-bootstrap": "^2.1.2",
    "react-chartjs-2": "^4.0.1",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "reactstrap": "^8.10.0",
    "typeorm": "^0.2.44",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.21",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/react-router-dom": "^5.3.3",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "concurrently": "^7.0.0",
    "electron-builder": "^22.14.13",
    "eslint": "^8.11.0",
    "eslint-plugin-react": "^7.29.4",
    "prettier": "^2.6.0",
    "typescript": "^4.6.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version"
    ]
  }
}
